<?xml version="1.0"?>

<PropertyList>

 <path>flash.ac</path>

 <animation>
  <type>noshadow</type>
  <object-name>flash</object-name>
 </animation>

 <animation>
  <type>billboard</type>
  <spherical>true</spherical>
 </animation>

 <animation>
  <type>select</type>
  <object-name>flash</object-name>
  <condition>
	  <and>
		   <greater-than>
		      <property>sim/multiplay/generic/float[0]</property>
		      <value>0.96</value>
		   </greater-than>
		   <property>controls/lighting/Strobe</property>
	  </and>
  </condition>
 </animation>

 <animation>
  <type>dist-scale</type>
  <object-name>flash</object-name>
  <interpolation>
   <entry><ind>0</ind><dep>1.0</dep></entry>
   <entry><ind>500</ind><dep>2.0</dep></entry>
   <entry><ind>16000</ind><dep>16.0</dep></entry>
  </interpolation>
 </animation>

</PropertyList>
